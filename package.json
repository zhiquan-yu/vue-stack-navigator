{
  "name": "vue-stack-navigator",
  "version": "0.0.1",
  "description": "The stack navigator for Vue apps",
  "keywords": [
    "vue",
    "stack",
    "navigator",
    "router"
  ],
  "author": "zhiquan",
  "license": "MIT",
  "main": "dist/vue-stack-navigator.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "release": "bash scripts/release.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@vue/composition-api": "^0.2.1",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "rollup": "^1.20.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.0",
    "rollup-plugin-vue": "^5.0.1",
    "typescript": "^3.6.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.2",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "@vue/composition-api": "0.2.x",
    "vue": "2.x",
    "vue-router": "3.x"
  }
}
